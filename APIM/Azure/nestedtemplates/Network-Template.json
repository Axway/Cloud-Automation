{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vnetName": {
            "type": "string"
        },
        "vnetId": {
            "type": "string"
        },
        "vnetPrefix": {
            "type": "string"
        },
        "snetAksName": {
            "type": "string"
        },
        "snetAksPrefix": {
            "type": "string"
        },
        "snetBastionName": {
            "type": "string"
        },
        "snetBastionPrefix": {
            "type": "string"
        },
        "snetDataName": {
            "type": "string"
        },
        "snetDataPrefix": {
            "type": "string"
        },
        "uriNsgTemplate": {
            "type": "string"
        }
    },
    "variables": {
        "nsgBastionName": "bastion-nsg",
        "nsgBastionId" : "[resourceId('Microsoft.Network/networkSecurityGroups', variables('nsgBastionName'))]",
        "nsgAksName": "aks-nsg",
        "nsgAksId" : "[resourceId('Microsoft.Network/networkSecurityGroups', variables('nsgAksName'))]",
        "nsgDataName": "data-nsg",
        "nsgDataId" : "[resourceId('Microsoft.Network/networkSecurityGroups', variables('nsgDataName'))]"
    },
    "resources": [
        {
            "type": "Microsoft.Network/virtualNetworks",
            "apiVersion": "2018-12-01",
            "name": "[parameters('vnetName')]",
            "location": "[resourceGroup().location]",
            "dependsOn": [],
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "[parameters('vnetPrefix')]"
                    ]
                },
                "subnets": [
                    {
                        "name": "[parameters('snetAksName')]",
                        "properties": {
                            "addressPrefix": "[parameters('snetAksPrefix')]",
                            "networkSecurityGroup": {
                                "id": "[variables('nsgAksId')]"
                            },
                            "serviceEndpoints": [
                                {
                                    "service": "Microsoft.Storage",
                                    "locations": [
                                        "[resourceGroup().location]"
                                    ]
                                },
                                {
                                    "service": "Microsoft.ContainerRegistry",
                                    "locations": [
                                        "*"
                                    ]
                                },
                                {
                                    "service": "Microsoft.Sql",
                                    "locations": [
                                        "[resourceGroup().location]"
                                    ]
                                }
                            ],
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Enabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        }
                    },
                    {
                        "name": "[parameters('snetDataName')]",
                        "properties": {
                            "addressPrefix": "[parameters('snetDataPrefix')]",
                            "networkSecurityGroup": {
                                "id": "[variables('nsgDataId')]"
                            },
                            "serviceEndpoints": [
                                {
                                    "service": "Microsoft.Storage",
                                    "locations": [
                                        "[resourceGroup().location]"
                                    ]
                                }
                            ],
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Enabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        }
                    },
                    {
                        "name": "[parameters('snetBastionName')]",
                        "properties": {
                            "addressPrefix": "[parameters('snetBastionPrefix')]",
                            "networkSecurityGroup": {
                                "id": "[variables('nsgBastionId')]"
                            },
                            "serviceEndpoints": [
                                {
                                    "service": "Microsoft.Storage",
                                    "locations": [
                                        "[resourceGroup().location]"
                                    ]
                                },
                                {
                                    "service": "Microsoft.Sql",
                                    "locations": [
                                        "[resourceGroup().location]"
                                    ]
                                }
                            ],
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Enabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        }
                    }
                ],
                "virtualNetworkPeerings": [],
                "enableDdosProtection": false,
                "enableVmProtection": false
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2019-09-01",
            "name": "[concat(parameters('vnetName'), '/', parameters('snetAksName'))]",
            "dependsOn": [
                "NSG",
                "[parameters('vnetId')]"
            ],
            "properties": {
                "addressPrefix": "[parameters('snetAksPrefix')]",
                "networkSecurityGroup": {
                    "id": "[variables('nsgAksId')]"
                },
                "serviceEndpoints": [
                    {
                        "service": "Microsoft.Storage",
                        "locations": [
                            "[resourceGroup().location]"
                        ]
                    },
                    {
                        "service": "Microsoft.ContainerRegistry",
                        "locations": [
                            "*"
                        ]
                    },
                    {
                        "service": "Microsoft.Sql",
                        "locations": [
                            "[resourceGroup().location]"
                        ]
                    }
                ],
                "delegations": []
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2019-09-01",
            "name": "[concat(parameters('vnetName'), '/', parameters('snetDataName'))]",
            "dependsOn": [
                "NSG",
                "[parameters('vnetId')]",
                "[resourceId('Microsoft.Network/virtualNetworks/subnets', parameters('vnetName'),parameters('snetBastionName'))]"

            ],
            "properties": {
                "addressPrefix": "[parameters('snetDataPrefix')]",
                "networkSecurityGroup": {
                    "id": "[variables('nsgDataId')]"
                },
                "serviceEndpoints": [
                    {
                        "service": "Microsoft.Storage",
                        "locations": [
                            "[resourceGroup().location]"
                        ]
                    }
                ],
                "delegations": []
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2019-09-01",
            "name": "[concat(parameters('vnetName'), '/', parameters('snetBastionName'))]",
            "dependsOn": [
                "NSG",
                "[parameters('vnetId')]",
                "[resourceId('Microsoft.Network/virtualNetworks/subnets', parameters('vnetName'),parameters('snetAksName'))]"
            ],
            "properties": {
                "addressPrefix": "[parameters('snetBastionPrefix')]",
                "networkSecurityGroup": {
                    "id": "[variables('nsgBastionId')]"
                },
                "serviceEndpoints": [
                    {
                        "service": "Microsoft.Storage",
                        "locations": [
                            "[resourceGroup().location]"
                        ]
                    },
                    {
                        "service": "Microsoft.ContainerRegistry",
                        "locations": [
                            "*"
                        ]
                    },
                    {
                        "service": "Microsoft.Sql",
                        "locations": [
                            "[resourceGroup().location]"
                        ]
                    }
                ],
                "delegations": []
            }
        }
    ]
}